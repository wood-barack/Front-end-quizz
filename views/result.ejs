<%- include('pages/header'); -%>          
<body class="hold-transition sidebar-mini">
  <div class="wrapper">
  
    <!-- Navbar -->
    <%- include('pages/navbar'); -%>
    <!-- /.navbar -->
  
    <!-- Main Sidebar Container -->
    <aside class="main-sidebar sidebar-dark-primary elevation-4">
      <!-- Brand Logo -->
      <a href="home" class="brand-link">
        <!-- <img src="/assets/img/wood.svg" alt="AdminLTE Logo" class="brand-image img-circle elevation-2"
              style="opacity: .8"> -->
          <img src="/assets/img/wood.svg" class="img-circle elevation-2" alt="User Image" style="width: 10%;">
          <span class="brand-text font-weight-light">Barack-Quizz</span>
      </a>
  
      <!-- Sidebar -->
      <div class="sidebar">
        <!-- Sidebar user panel (optional) -->
        <%- include('pages/sidebar'); -%>
        
      <!-- /.sidebar -->
    </aside>
  
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <div class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1 class="m-0 text-dark">Mes resultat</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
              <ol class="breadcrumb float-sm-right">
                <li class="breadcrumb-item"><a href="">Mes resultat</a></li>
                <li class="breadcrumb-item active">Participation au quizz</li>
              </ol>
            </div><!-- /.col -->
          </div><!-- /.row -->
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content-header -->
      <br>
      <!-- Main content -->
      <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <div class="info-box">
                    <span class="info-box-icon bg-success"><i class="fas fa-trophy"></i></span>
        
                    <div class="info-box-content">
                        <span class="info-box-text">Mon score</span>
                        <span class="info-box-number"><%= data.score %></span>
                    </div>
                    </div>
                    
                </div>
                <div class="col-md-3"></div>                     
            </div>
            <br>
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <div class="info-box">
                        <span class="info-box-icon bg-success"><i class="far fa-hourglass"></i></span>
          
                        <div class="info-box-content">
                          <span class="info-box-text">Temps Moyens Reponse</span>
                          <span class="info-box-number"><%= data.tempsMoyenReponse %> secondes</span>
                        </div>
                        <!-- /.info-box-content -->
                      </div>
                    
                </div>
                <div class="col-md-3"></div>                     
            </div>
            
            <div class="row">
                <div class="col-md-3"> </div>
                <div class="col-md-4">
                    <a href="/Quizz" class="btn btn-primary" id=''>Re-jouer</a>                    
                </div>
                <div class="col-md-4">
                    <a href="/home" class="btn btn-primary">Home</a>
                    
                </div>
                <!-- <div class="col-md-2"></div>                      -->
            </div>
                <!-- ./col -->
        </div>
              
          <!-- /.row -->
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
  
    <!-- Control Sidebar -->
    <aside class="control-sidebar control-sidebar-dark">
      <!-- Control sidebar content goes here -->
      <div class="p-3">
        <h5>Title</h5>
        <p>Sidebar content</p>
      </div>
    </aside>
    <!-- /.control-sidebar -->
    
  </div>

  <script>
    
  </script>

<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>

<%- include('pages/footer'); -%>